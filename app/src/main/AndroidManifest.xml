<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="mera.jobsearch">
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="mera.jobsearch.SettingsActivity"
            android:label="@string/title_activity_settings"></activity>
        <activity android:name="mera.jobsearch.AdminActivity" />
        <activity android:name="mera.jobsearch.Launch_screenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="mera.jobsearch.Home_screenActivity"
            android:label="Homescreen" />
    </application>

</manifest>